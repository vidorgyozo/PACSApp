<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.*?>
<GridPane fx:id="gridPane" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pacsapp.pacsapp.controller.DownloadController">
<children>
    <Label fx:id="rspLabel" text="" wrapText="true" GridPane.columnSpan="2" GridPane.rowIndex="2">
        <GridPane.margin>
            <Insets left="30.0" />
        </GridPane.margin>
    </Label>
    <Button fx:id="backButton" mnemonicParsing="false" onAction="#handleToFindAction" prefHeight="50.0" prefWidth="260.0" text="BACK" GridPane.halignment="LEFT" GridPane.rowIndex="3">
        <GridPane.margin>
            <Insets left="30.0" top="10.0" />
        </GridPane.margin>
    </Button>
    <Button fx:id="downloadButton" mnemonicParsing="false" onAction="#handleDownloadAction" prefHeight="50.0" prefWidth="260.0" text="DOWNLOAD" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3">
        <GridPane.margin>
            <Insets right="30.0" top="10.0" />
        </GridPane.margin>
    </Button>
      <ProgressIndicator fx:id="progressIndicator" visible="false" maxWidth="40.0" minWidth="40.0" prefWidth="40.0" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets right="30.0" />
         </GridPane.margin>
      </ProgressIndicator>
</children>
<rowConstraints>
    <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" />
    <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="600.0" />
    <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="60.0" />
    <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="60.0" />
    <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" />
</rowConstraints>
<columnConstraints>
    <ColumnConstraints maxWidth="-Infinity" minWidth="-Infinity" prefWidth="300.0" />
    <ColumnConstraints maxWidth="-Infinity" minWidth="-Infinity" prefWidth="300.0" />
</columnConstraints>
</GridPane>
